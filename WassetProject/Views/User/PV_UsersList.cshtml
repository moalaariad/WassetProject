@model List<WassetPortal_DAL.Models.User>
<button class="btn btn-primary" onclick="window.location.href = '/user/createUser'">Create User</button>
<table class="table table-striped table-dark col-md-9">
    <thead>
        <tr>
            <th scope="col">User ID</th>
            <th scope="col">User Code</th>
            <th scope="col">User Name</th>
            <th scope="col">Mobile</th>
            <th scope="col">Email</th>
            <th scope="col">Address</th>
            <th scope="col">User Status</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (Model != null)
            {
                foreach (var user in Model)
                {
                    <tr>
                        <th scope="row">@user.UserID</th>
                        <td>@user.UserCode</td>
                        <td>@user.UserName</td>
                        <td>@user.Mobile</td>
                        <td>@user.Email</td>
                        <td>@user.Address</td>
                        @{ if (user.UserStatus == true)
                            {
                                <td>
                                    Active
                                </td>
                            }
                            else
                            {
                                <td>
                                    Not Active
                                </td>
                            }
                        }
                        <td></td>
                    </tr>
                }
            }
        }

    </tbody>
</table>
